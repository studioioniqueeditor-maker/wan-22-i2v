# Application Settings
FLASK_SECRET_KEY=your_secret_key_here
FLASK_ENV=production

# Supabase Configuration (Authentication & Database)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# Google Cloud Configuration (Storage & Vertex AI)
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id
GOOGLE_CLOUD_LOCATION=us-central1
GCS_BUCKET_NAME=your_gcs_bucket_name
# Note: Google Cloud credentials (gcs-key.json) should be mounted or set via GOOGLE_APPLICATION_CREDENTIALS path

# RunPod Configuration (Wan 2.1 Model)
RUNPOD_API_KEY=your_runpod_api_key
RUNPOD_ENDPOINT_ID=your_runpod_endpoint_id

# Groq Configuration (Prompt Enhancement - currently disabled in UI)
GROQ_API_KEY=your_groq_api_key

# Development Settings
MOCK_MODE=false